{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "fakerapi.it/api/v1/credit_cards schema",
  "type": "object",
  "properties": {
    "status": {
      "type": "string",
      "pattern": "^OK$"},
    "code": { "type": "integer" },
    "total": { "type": "integer" },
    "data": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "type": { "type": "string" },
          "number": { "type": "string" },
          "expiration": { "type": "string" },
          "owner": {
            "type": "string",
            "minLength": 2}
        },
        "required": ["type", "number", "expiration", "owner"]
      }
    }
  },
  "required": ["status", "code", "total", "data"]
}
